    "test:e2e": "vitest run actions.e2e.test.ts",
    "demo": "node --import tsx examples/demo.ts",
    "check-token": "node --import tsx examples/token-check.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.0.1",
    "@types/node-fetch": "^2.6.12",
    "node-fetch": "^3.3.2",
