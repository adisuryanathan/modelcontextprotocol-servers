    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "dotenv": "^16.4.1",
    "jsdom": "^26.0.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.6.2",
    "vitest": "^3.0.8"
  }
